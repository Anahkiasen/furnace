{
  "name": "anahkiasen/furnace",
  "description": "An app to rate Rocksmith CDLCs",
  "keywords": [
    "rocksmith",
    "cdlc"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "5.0.*",
    "league/csv": "~7.0",
    "guzzlehttp/guzzle": "~5.2",
    "rcrowe/twigbridge": "0.7.*",
    "symfony/dom-crawler": "~2.6",
    "symfony/css-selector": "~2.6",
    "laravelcollective/annotations": "~5.0",
    "guidovanbiemen/laravel-whoops": "~1.0",
    "anahkiasen/former": "4.0.*@dev",
    "anahkiasen/arrounded": "0.6.*",
    "laravel/socialite": "~2.0",
    "mathiasgrimm/laravel-dot-env-gen": "0.8.*",
    "creativeorange/gravatar": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "laracasts/generators": "~1.1",
    "symfony/var-dumper": "~2.6",
    "barryvdh/laravel-debugbar": "~2.0",
    "barryvdh/laravel-ide-helper": "~2.0",
    "doctrine/dbal": "~2.5",
    "benconstable/phpspec-laravel": "~2.0",
    "league/factory-muffin": "~2.1",
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Furnace\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\Furnace\\": "tests/spec"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
